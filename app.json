{
  "expo": {
    "name": "Mavrixfy",
    "slug": "mavrixfy",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mavrixfy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mavrixfy.app",
      "buildNumber": "2",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSMicrophoneUsageDescription": "This app may use the microphone for voice features.",
        "NSCameraUsageDescription": "This app uses the camera to upload profile pictures.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload images.",
        "NSAppleMusicUsageDescription": "This app streams music content.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.mavrixfy.app",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "backgroundColor": "#121212",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE"
      ],
      "enableDangerousExperimentalLeanBuilds": true,
      "navigationBar": {
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://replit.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-system-ui",
      [
        "expo-updates",
        {
          "username": "satvik1234"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "targetSdkVersion": 35,
            "compileSdkVersion": 35,
            "enableShrinkResourcesInReleaseBuilds": true,
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.facebook.hermes.unicode.** { *; }\n-keep class com.facebook.jni.** { *; }\n-keep class com.doublesymmetry.** { *; }\n-keep class expo.modules.updates.** { *; }\n-keep class kotlin.** { *; }\n-keep class kotlin.reflect.** { *; }",
            "useLegacyPackaging": false,
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so"
              ],
              "exclude": [
                "META-INF/*.kotlin_module"
              ]
            },
            "enableMinifyInReleaseBuilds": true,
            "usesCleartextTraffic": false,
            "abiFilters": ["armeabi-v7a"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "web": {
      "bundler": "metro"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "url": "https://u.expo.dev/93293119-93b7-4dbb-acdd-7241771254c4",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": "1.2.0",
    "extra": {
      "router": {
        "origin": "https://replit.com/"
      },
      "domain": "spotify-api-drab.vercel.app",
      "firebaseApiKey": "AIzaSyBWgv_mE8ZAnG2kUJSacCOUgkbo1RxxSpE",
      "firebaseAuthDomain": "spotify-8fefc.firebaseapp.com",
      "firebaseProjectId": "spotify-8fefc",
      "firebaseStorageBucket": "spotify-8fefc.firebasestorage.app",
      "firebaseMessagingSenderId": "816396705670",
      "firebaseAppId": "1:816396705670:web:005e724df7139772521607",
      "firebaseMeasurementId": "G-FQJS8LREP5",
      "eas": {
        "projectId": "93293119-93b7-4dbb-acdd-7241771254c4"
      }
    },
    "owner": "satvik1234"
  }
}
