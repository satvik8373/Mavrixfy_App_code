{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "channel": "production",
      "autoIncrement": false,
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UPDATES": "1",
        "EAS_NO_VCS": "1"
      }
    },
    "production-arm": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UPDATES": "1"
      }
    },
    "production-optimized": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UPDATES": "1",
        "EXPO_NO_FLIPPER": "1",
        "EXPO_NO_CLIENT_SIDE_ENV": "1"
      }
    },
    "production-aab": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UPDATES": "1",
        "EXPO_NO_FLIPPER": "1",
        "EXPO_NO_CLIENT_SIDE_ENV": "1"
      }
    },
    "production-arm64": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_NO_DOTENV": "1"
      }
    },
    "production-ultra-small": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      },
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_NO_DOTENV": "1",
        "EXPO_NO_CLIENT_SIDE_ENV": "1",
        "EXPO_NO_FLIPPER": "1"
      }
    },
    "production-universal": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UPDATES": "1",
        "EXPO_NO_FLIPPER": "1"
      }
    },
    "production-split": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_UPDATES": "1",
        "EXPO_NO_FLIPPER": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
