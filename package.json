{
  "name": "expo-app",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 npx expo start --localhost",
    "server:dev": "set NODE_ENV=development&& tsx server/index.ts",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "set NODE_ENV=production&& node server_dist/index.js",
    "db:push": "drizzle-kit push",
    "start": "npx expo start",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "generate:icons": "node scripts/generate-app-icons.js",
    "fix:deps": "node scripts/fix-dependencies.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "version:major": "node scripts/bump-version.js major",
    "version:minor": "node scripts/bump-version.js minor",
    "version:patch": "node scripts/bump-version.js patch",
    "test:api": "node scripts/test-api-connection.js",
    "build:apk": "cd android && .\\gradlew assembleRelease",
    "build:eas": "npx eas build --platform android --profile production",
    "build:aab": "npx eas build --platform android --profile production-aab",
    "update:production": "npx eas update --branch production",
    "update:preview": "npx eas update --branch preview",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@shopify/flash-list": "^2.0.2",
    "@tanstack/react-query": "^5.90.21",
    "babel-plugin-module-resolver": "^5.0.0",
    "expo": "~54.0.33",
    "expo-auth-session": "^7.0.10",
    "expo-blur": "^15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.11",
    "expo-crypto": "^15.0.8",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-keep-awake": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "^29.0.16",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.9.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-track-player": "^4.1.2",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@types/express": "^5.0.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~10.0.0",
    "express": "^5.0.1",
    "http-proxy-middleware": "^3.0.5",
    "patch-package": "^8.0.1",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "ws": "^8.18.0"
  },
  "private": true
}
